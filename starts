import App from "@system.app";


// 跳转页面入口函数
@Builder
export function startsBuilder() {
  starts();
}

@Component
struct starts {
  pathStack: NavPathStack = new NavPathStack();
aboutToAppear(): void {
  setTimeout(
    ()=>{this.pathStack.replacePathByName("layouts", null, false);},3000
  )
}
  build() {
    NavDestination() {
      Stack({alignContent:Alignment.TopEnd})
      {
Image($r("app.media.hmmusicad"))
  .width("100%")
  .height("100%")
  .expandSafeArea([SafeAreaType.SYSTEM],[SafeAreaEdge.TOP,SafeAreaEdge.BOTTOM])
  Button("跳过")
    .backgroundColor(Color.Gray)
    .margin(15)
    .onClick(()=>{this.pathStack.replacePathByName("layouts", null, false);})
      }
    }


    .onReady((context: NavDestinationContext) => {
      this.pathStack = context.pathStack;
    })
  }
}
